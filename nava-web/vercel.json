  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install && npx playwright install chromium",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/**/*.ts": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "env": {
    "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "0"
  }
}
